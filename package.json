{
    "name": "ngxs-data",
    "version": "0.0.0",
    "scripts": {
        "preinstall": "node -e \"if(process.env.npm_execpath.indexOf('yarn') === -1) throw new Error('You must use Yarn to install, not NPM')\"",
        "start": "ng serve --open",
        "build:app": "ng build --prod",
        "build:lib": "ng build ngxs-data && ts-node --project tsconfig.tools.json ./tools/copy-readme",
        "test": "jest --config ./jest.app.config.js",
        "test:ci": "jest --config ./jest.app.config.js --coverage",
        "lint": "eslint '*.ts'",
        "coverage": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
        "format": "prettier --write \"**/*.{js,ts,html,css,scss,md,json,yml}\""
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "8.2.14",
        "@angular/common": "8.2.14",
        "@angular/compiler": "8.2.14",
        "@angular/core": "8.2.14",
        "@angular/forms": "8.2.14",
        "@angular/platform-browser": "8.2.14",
        "@angular/platform-browser-dynamic": "8.2.14",
        "@angular/router": "8.2.14",
        "@ngxs/logger-plugin": "3.6.0",
        "@ngxs/store": "3.6.0",
        "core-js": "3.6.1",
        "rxjs": "6.5.3",
        "tslib": "1.10.0",
        "zone.js": "0.10.2"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "0.803.21",
        "@angular-devkit/build-ng-packagr": "0.803.21",
        "@angular/cli": "8.3.21",
        "@angular/compiler-cli": "8.2.14",
        "@angular/language-service": "8.2.14",
        "@types/jest": "24.0.24",
        "@types/node": "12.7.1",
        "@typescript-eslint/eslint-plugin-tslint": "2.13.0",
        "@typescript-eslint/eslint-plugin": "2.13.0",
        "@typescript-eslint/parser": "2.13.0",
        "angular-cli-ghpages": "0.6.1",
        "codelyzer": "5.2.1",
        "colors": "1.4.0",
        "eslint": "6.8.0",
        "jasmine-core": "3.5.0",
        "jasmine-spec-reporter": "4.2.1",
        "jest": "24.9.0",
        "jest-preset-angular": "7.1.1",
        "ng-packagr": "5.7.1",
        "prettier": "1.19.1",
        "ts-node": "8.5.4",
        "tsickle": "0.37.1",
        "tslint": "5.20.1",
        "coveralls": "3.0.9",
        "typescript": "3.5.1"
    }
}
